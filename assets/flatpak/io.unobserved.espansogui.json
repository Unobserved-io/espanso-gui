{
  "app-id": "io.unobserved.espansoGUI",
  "runtime": "org.freedesktop.Platform",
  "runtime-version": "23.08",
  "sdk": "org.freedesktop.Sdk",
  "sdk-extensions": [
    "org.freedesktop.Sdk.Extension.rust-stable"
  ],
  "command": "espansogui",
  "finish-args": [
    "--share=ipc",
    "--socket=fallback-x11",
    "--device=dri",
    "--socket=wayland",
    "--filesystem=xdg-xdg-config/espanso"
  ],
  "build-options": {
    "append-path": "/usr/lib/sdk/rust-stable/bin"
  },
  "modules": [
    {
      "name": "espansogui",
      "builddir": true,
      "buildsystem": "simple",
      "sources": [
        {
          "type": "dir",
          "path": "./"
        }
      ]
    }
  ]
}
